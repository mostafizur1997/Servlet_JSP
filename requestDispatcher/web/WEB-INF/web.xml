<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Servlet1</servlet-name>
        <servlet-class>com.learn.Servlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet2</servlet-name>
        <servlet-class>com.learn.Servlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>profileServ</servlet-name>
        <servlet-class>com.learn.profileServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet1</servlet-name>
        <url-pattern>/Servlet1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet2</servlet-name>
        <url-pattern>/Servlet2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>profileServ</servlet-name>
        <url-pattern>/profileServ</url-pattern>
    </servlet-mapping>
    
    
    
    <welcome-file-list>  
   <welcome-file>index.html</welcome-file>  
   </welcome-file-list>
  
  
  <welcome-file-list>  
   <welcome-file>profile1.html</welcome-file>  
  </welcome-file-list>
  
  
  
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
